<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.kde.networkmanagement">
    <!-- Re-read configuration -->
    <method name="ListConnectables">
      <arg name="connectables" type="ao" direction="out"/>
    </method>
    
    <signal name="ConnectableAdded">
	  <arg name="changedConnectable" type="o" direction="out" />
    </signal>
    <signal name="ConnectableRemoved">
	  <arg name="changedConnectable" type="o" direction="out" />
    </signal>
  </interface>
</node>
